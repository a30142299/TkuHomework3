<?php
echo "<table border='1'>";
	for($i=1 ; $i <= 9 ; $i++){
		echo"<tr>";
		for($j = 1 ; $j<=9 ; $j++){
			$k=$i*$j;
			echo"<td>$i*$j=$k</td>";
		}
		echo"</tr>";
	}
echo"</table>";
?>
