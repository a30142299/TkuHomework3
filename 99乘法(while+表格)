<?php
echo "<table border='1'>";
$i=1;
	while($i<=9){
		echo"<tr>";
		$j=1;
		while($j<=9){
			$k=$i*$j;
			echo "<td>$i*$j=$k</td>";
			$j++;
		}
		echo "</tr>";
		$i++;
	}
echo"</table>";
?>
</body>
